<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper	namespace="com.example.demo.webapp.dao.AddrDao">


    <select id="findByWhere" parameterType="Addr" resultType="Addr" useCache="true">
        select * from GF_ADDR
        <where>
            <if test = "gcaId != null">
                and gca_id > #{gca_id}
            </if>
        </where>
    </select>

</mapper>
